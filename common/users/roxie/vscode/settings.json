{
    ////// Editor Styling
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.suggestSelection": "first",
    // Edit tabsizes to make html and json have 2 but most other languages use 4
    "editor.tabSize": 4,
    "[html]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 2
    },
    "[nix]": {
        "editor.tabSize": 2
    },
    "editor.insertSpaces": true,
    // Colour Theme Settings
    "workbench.colorTheme": "Dracula",
    // Overriding colour scheme to add some stuff most schemes don't have annoyingly
    "editor.tokenColorCustomizations": {
        "[Dracula]": {
            // Change comments to be more readable for this theme
            "textMateRules": [
                {
                    "scope": [
                        "comment.line",
                        "comment.block.documentation",
                        "string.quoted.docstring.multi"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#90a7ee"
                    }
                }
            ]
        },
        "[Monokai Sweet Dark]": {
            "textMateRules": [
                {
                    "scope": [
                        "variable.parameter.function.language.special.self.python",
                        "variable.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#7d61b3",
                    }
                },
    
            ]
        },
        "[Monokai Sweet Darker]": {
            "textMateRules": [
                {
                    "scope": [
                        "variable.parameter.function.language.special.self.python",
                        "variable.language.special.self.python"
                    ],
                    "settings": {
                        "foreground": "#7d61b3",
                    }
                },
    
            ]
        }
    },
    ////// UI and Program Settings
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDelete": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.associations": {
        "*.go": "go",
        "*.html": "html",
        ".style.yapf": "ini"
    },
    
    // Remove a lot of remote MS stuff
    "telemetry.enableCrashReporter": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.showReleaseNotes": false,
    "telemetry.enableTelemetry": false,
    
    ////// Terminal Settings (Adding Kitty Support)
    "terminal.external.linuxExec": "kitty",
    ////// Git Settings
    "git.allowForcePush": true,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    // Git Lens Ext
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.search.enabled": false,
    "gitlens.views.compare.enabled": false,
    
    ////// Python Settings
    "python.jediEnabled": false,
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "python.linting.pylintUseMinimalCheckers": false,
    "python.formatting.provider": "yapf",
    // AutoDocString ext
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.includeName": true,
    
    ////// Docker Settings
    "docker.dockerComposeDetached": false,
    "docker.dockerComposeBuild": false,
    "docker.containers.label": "ContainerName",
    
    ////// GoLang Settings
    "go.gopath": "/home/roxie/Projects/go",
    "go.formatTool": "goimports",
    "go.lintOnSave": "package",
    "go.useLanguageServer": false,
    "go.toolsGopath": "/home/roxie/Projects/go",
    "go.autocompleteUnimportedPackages": true,
    
